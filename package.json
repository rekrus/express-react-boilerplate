{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "client-start": "cd client && npm start",
    "app": "concurrently npm:dev npm:client-start --kill-others",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "client-lint": "cd client && npm run lint",
    "app-lint": "concurrently npm:lint npm:client-lint"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^3.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "author": "Rekrus",
  "repository": {
    "type": "git",
    "url": "https://github.com/rekrus/express-react-boilerplate"
  },
  "license": "MIT"
